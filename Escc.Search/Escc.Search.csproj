<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.0.0</Version>
    <Authors>East Sussex County Council</Authors>
    <Description>Interface and controls for requesting and displaying search results. Implemented for specific search providers, eg Escc.Search.Google.</Description>
    <Copyright>East Sussex County Council</Copyright>
    <PackageLicenseUrl>http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/</PackageLicenseUrl>
    <PackageIconUrl>https://www.eastsussex.gov.uk/apple-touch-icon.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/east-sussex-county-council/Escc.Search</PackageProjectUrl>
    <PackageTags>search</PackageTags>
    <RepositoryUrl>https://github.com/east-sussex-county-council/Escc.Search</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en-GB</NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageReleaseNotes>2.0.0 targets .NET Standard and removes the strong name</PackageReleaseNotes>
  </PropertyGroup>

</Project>
